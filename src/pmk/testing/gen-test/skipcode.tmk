%%OPTIONS
    Library '../../../../lib/ansi-c';
    Prefix 'skipcode';
%%IMPORT
typedef int TmNatural;     /* A natural number of "reasonable" length. */
typedef int TmCode;        /* The type of a token code. */
%%END

%%SRCP Name TmSrcp;
    Row line %% TmNatural %1 %%;
    Column col %% TmNatural %1 %%;
    File file %% TmNatural %1 %%;
%%END

%%TOKEN Name TmToken;
    Code code %% TmCode %1 %%;
    Srcp srcp %% TmSrcp %1 %%;
    Attributes
        stringValue %% char %1[256] %%,
        integerValue %% int %1 %%;
%%END
